{
  "name": "puppeteer-middlewares",
  "version": "1.0.0",
  "description": "A collection of middlewares for Puppetteer PageEvent",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/teocns/puppeteer-middlewares",
  "author": "teocns",
  "scripts": {
    "clean-dist": "rimraf dist",
    "test": "jest",
    "generate-d-ts": "api-extractor run --local --verbose",
    "tsc-cjs": "tsc -b src/tsconfig.cjs.json",
    "tsc-esm": "tsc -b src/tsconfig.esm.json",
    "lint": "eslint . --ext .ts -f",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run clean-dist && tsc --build tsconfig.json --force",
    "clean": "rm -rf ./node-modules && rm -f tsconfig.tsbuildinfo && rm -f",
    "compile": "npm run clean && npm install && npm run build"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "@testdeck/mocha": "^0.2.1",
    "chai": "^4.3.6",
    "got-cjs": "^12.3.1",
    "http-proxy-agent": "*",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "puppeteer": "^19.1.0",
    "socks-proxy-agent": "*",
    "tough-cookie": "*",
    "ts-mixer": "^6.0.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3"
  }
}
